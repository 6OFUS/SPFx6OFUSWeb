{
  "hosting": {
    "public": "out",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
        {
          "source": "/home",
          "destination": "/home.html"
        },
        {
          "source": "/events",
          "destination": "/events.html"
        }
      ]
  }
}
