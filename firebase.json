{
  "hosting": {
    "public": "out",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
        {
          "source": "/events",
          "destination": "/events.html"
        },
        {
          "source": "/aboutus",
          "destination": "/aboutus.html"
        },
        {
          "source": "/game",
          "destination": "/game.html"
        },
        {
          "source": "/feedback",
          "destination": "/feedback.html"
        },
        {
          "source": "/scam",
          "destination": "/scam.html"
        },
        {
          "source": "/shoptheft",
          "destination": "/shoptheft.html"
        },
        {
          "source": "/credits",
          "destination": "/credits.html"
        }
      ]
  }
}
